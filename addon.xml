<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="service.madeo.rs232server"
       name="Rs232Server"
       version="0.4"
       provider-name="Brendan Le Foll">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
  </requires>
  <extension point="xbmc.service"
             library="default.py" start="login|startup">
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Provides integration with rs232server for amplifier/tv control over RS232.</summary>
  </extension>
</addon>
